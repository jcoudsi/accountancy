entity Invoice {    code String required    date Instant required    details String    status InvoiceStatus required    paymentMethod PaymentMethod required    paymentDate Instant required    paymentAmount BigDecimal required}
enum InvoiceStatus {    PAID, ISSUED, CANCELLED}
entity Shipment {    trackingCode String    date Instant required    details String}
enum PaymentMethod {    CREDIT_CARD, CASH_ON_DELIVERY, PAYPAL}
relationship OneToMany {    Invoice{shipment} to Shipment{invoice(code) required}}
service Invoice, Shipment with serviceClass
paginate Invoice, Shipment with pagination
microservice Invoice, Shipment with accountancy